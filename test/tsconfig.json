{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": ".",
    "outDir": "build",
    "paths": {
      "froggy-interpreter": [".."],
      "froggy": ["../example/node_modules/froggy"]
    }
  },
  "exclude": [
    "!/node_modules/@types",
    "../node_modules",
    "../example",
    "../src/"
  ]
}
